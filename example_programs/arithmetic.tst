///////////////              ARITHMETIC                 ///////////////
///////////////////////////////////////////////////////////////////////

let x = (5 + 2) * 43 - 3
print(x) // prints 298

// powers
let x = 5^2
print(x) // prints 25

// modulus brackets
let x = |-10|
print(x) // prints 10
let x = |23 - 32 * 56|
print(x) // prints 1769
print|23 - 32 * 56| // prints 1769